{
  "name": "protractor-huge-automation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "debug": "node --inspect-brk ./node_modules/protractor/bin/protractor ./e2e/protractor.conf.js",
    "dev": "node_modules/protractor/bin/protractor e2e/protractor.conf.js",
    "webdriver-update": "node_modules/protractor/bin/webdriver-manager update",
    "browserStack": "node_modules/protractor/bin/protractor e2e/protractorStack.conf.js",
    "test": "npm run dev -- --suite",
    "generateReport": "$(cd e2e) allure generate e2e/allure-results --clean -o e2e/allure-report",
    "viewReport": "allure serve e2e/allure-results",
    "report": "npm run generateReport && npm run viewReport"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/jquery": "^3.3.31",
    "core-js": "^2.6.10",
    "jasmine-allure-reporter": "^1.0.2",
    "protractor-screenshot-utils": "^1.0.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "allure-commandline": "^2.13.0",
    "codelyzer": "~4.5.0",
    "eyes.selenium": "^3.8.2",
    "ffmpeg": "0.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.2",
    "protractor-video-reporter": "^0.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "prettier": {
    "singleQuote": true
  }
}
